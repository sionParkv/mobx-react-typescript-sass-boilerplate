{
  "name": "mobx-react-typescript-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for MobX + React project with Typescript, ES6, & HMR",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --hot --progress --env=local --mode=development",
    "build:dev": "webpack --config config/webpack.dev.js --progress --env=local --mode=development",
    "build:staging": "webpack --config config/webpack.stag.js --progress --profile --bail",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "analyze": "npm run build:prod && open ./dist/stats.html"
  },
  "author": "Jason Barnett <jason@thebarnetts.ws> (http://github.com/jhbarnett)",
  "repository": "https://github.com/jhbarnett/mobx-react-typescript-sass-boilerplate",
  "license": "MIT",
  "devDependencies": {
    "@types/es6-promise": "3.3.0",
    "@types/react": "16.14.21",
    "@types/react-dom": "16.9.14",
    "@types/react-loadable": "5.5.6",
    "@types/react-router-dom": "5.1.7",
    "@webpack-cli/serve": "^1.6.0",
    "css-loader": "5.2.7",
    "express": "4.17.2",
    "node-sass": "4.14.1",
    "sass-loader": "7.3.1",
    "style-loader": "2.0.0",
    "ts-loader": "8.3.0",
    "tslib": "2.3.1",
    "typescript": "4.3.4",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.46.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.3",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "es6-promise": "4.2.8",
    "mobx": "5.15.7",
    "mobx-observable-history": "1.0.3",
    "mobx-react": "5.4.4",
    "mobx-react-router": "4.1.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-loadable": "5.5.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.3.0"
  }
}
